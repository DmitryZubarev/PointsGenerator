{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "objects-topic",
    "ClientId": "object-generator",

    "ProducerSettings": {
      "Acks": "All",
      "MessageSendMaxRetries": 3,
      "RetryBackoffMs": 1000,
      "LingerMs": 5,
      "BatchSize": 32768,
      "EnableIdempotence": true,
      "CompressionType": "Snappy"
    },

    "ConsumerSettings": {
      "GroupId": "object-processor-group",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": false,
      "StatisticsIntervalMs": 5000
    },

    "SchemaRegistry": {
      "Url": "http://localhost:8081",
      "BasicAuthUserInfo": ""
    }
  }
}